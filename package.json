{
  "name": "razpi-electron-btc-ticker",
  "description": "Electron app built with Angular Material optimized for a Raspberry Pi that displays current stats about Bitcoin (BTC)",
  "license": "MIT",
  "version": "0.1.1",
  "author": {
    "name": "Andrew Schneider",
    "email": "andrew.mark.schneider@gmail.com",
    "url": "https://github.com/schnogz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/schnogz/razpi-electron-btc-ticker"
  },
  "bugs": {
    "url": "https://github.com/schnogz/razpi-electron-btc-ticker/issues"
  },
  "keywords": [
    "Electron",
    "Raspberry Pi",
    "Node.js",
    "AngularJS",
    "Angular Material",
    "Bitcoin",
    "Blockchain.Info"
  ],
  "main": "main.js",
  "scripts": {
    "start": "electron main.js ",
    "pack": "electron-packager . app --platform linux --arch x64 --out dist/ --overwrite",
    "deb-installer": "electron-installer-debian --src dist/app-linux-x64/ --dest dist/installers/ --arch amd64"
  },
  "dependencies": {
    "blockchain.info": "2.6.0",
    "body-parser": "1.16.0",
    "bower": "1.8.0",
    "chalk": "1.1.3",
    "electron-is-dev": "0.1.2",
    "errorhandler": "1.5.0",
    "express": "4.14.1",
    "express-status-monitor": "0.1.9",
    "morgan": "1.7.0"
  },
  "devDependencies": {
    "electron-installer-debian": "0.4.0",
    "electron-packager": "8.5.1",
    "electron-prebuilt": "1.4.13"
  }
}
